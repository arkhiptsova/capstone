                    <input variable="subject"/>
                </then>
                <else/>
            </if>
            <if expression="&quot;Would you like to see student's progress?&quot;">
                <then>
                    <output expression="&quot;ouput in the JPanel from Database: StudentID, firstName, lastName, date of submission and object of submission.&quot;" newline="True"/>
                </then>
                <else/>
            </if>
