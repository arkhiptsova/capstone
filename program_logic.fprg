    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="StudentID" type="String" array="False" size=""/>
            <declare name="firstName" type="String" array="False" size=""/>
            <declare name="lastName" type="String" array="False" size=""/>
            <output expression="&quot;Enter a student ID number&quot;" newline="True"/>
            <while expression="&quot;Student ID is not in Database&quot;">
                <output expression="&quot;Enter a correct student ID number&quot;" newline="True"/>
            </while>
            <if expression="&quot;Did student submit something?&quot;">
